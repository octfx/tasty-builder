{
  "name": "tasty-builder",
  "version": "1.0.0",
  "description": "A build UI tasty.js",
  "main": "builder.js",
  "types": "index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --mode=production",
    "build": "npm run lint:fix && npm run build:types && npm run build:js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/jsoneditor": "^5.28.2",
    "jsoneditor": "^9.5.6",
    "lodash": "^4.17.21",
    "paper": "^0.12.4",
    "tasty.js": "github:cosmoscout/tasty.js#f266fbf8ce7194903eaa5f7f5300a6bb2d9c10a5"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "babel-loader": "8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "fork-ts-checker-webpack-plugin": "4.0.1",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.12"
  }
}
